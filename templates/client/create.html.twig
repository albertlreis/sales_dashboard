{% extends 'base.html.twig' %}

{% block title %}Create Client{% endblock %}

{% block body %}
    <h1>Create Client</h1>

    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.phone) }}
    {{ form_row(form.email) }}

    <button type="submit">Create</button>
    {{ form_end(form) }}
{% endblock %}
